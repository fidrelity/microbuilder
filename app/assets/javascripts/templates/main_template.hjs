<div class="editor_content">

  <button {{action "showStage"}}> stage </button>
  <button {{action "showPaint"}}> paint </button>

  {{dynamicView editorContent}}
  
  {{#if overlayContent}}
  
    <div id="overlay">
    
      <h2>Overlay <button {{action "hideOverlay"}}> close </button></h2>
    
      {{dynamicView overlayContent}}
    
    </div>
    
    <div id="fog" {{action "hideOverlay"}}></div>
  
  {{/if}}

</div>
