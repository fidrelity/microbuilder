{{#if displayAll}}

  <h2>Placement</h2>

  <p>Drag your object to the right position</p>

{{/if}}

<div class="player">
  <canvas id="placementCanvas" width="640" height="390" style="background: #EEE;border:2px solid black;"></canvas>
</div>

{{#if displayAll}}

  <button {{action "cancel" target="App.gameController"}}>cancel</button>
  <button {{action "placeGraphic"}}>place</button>

{{/if}}
